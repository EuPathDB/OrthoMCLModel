<wdkModel>
  <stepAnalysisPlugins>

    <executionConfig threadPoolSize="25" fileStoreDirectory="@STEP_ANALYSIS_JOB_DIR@"/>

    <stepAnalysisPlugin
        name="sequence-cluster" displayName="Cluster Graph"
        customThumbnail="wdkCustomization/images/cluster-graph-logo.png"
        releaseVersion="22"
        formViewName="clusterGraphForm"
        analysisViewName="clusterGraphResult"
        analyzerClass="org.orthomcl.service.core.analysis.SequenceClusterAnalyzer">
      <shortDescription>
        <![CDATA[
          Cluster the sequences (no more than 500)
        ]]>
      </shortDescription>
      <description>
        <![CDATA[
          <p>Cluster the sequences in the result (only available for results with 500 sequences or fewer).
           </p>
        ]]>
      </description>
    </stepAnalysisPlugin>

  </stepAnalysisPlugins>
</wdkModel>
